    "google-auth-library": "^9.6.3",
    "shx": "^0.3.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.11",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "author": "CabLate",
  "files": [
    "dist",
    "dist/**/*.map",
    "README.md"
  ],
